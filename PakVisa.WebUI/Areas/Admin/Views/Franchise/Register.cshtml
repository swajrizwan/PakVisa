@model PakVisa.WebUI.Areas.Admin.Models.FranchiseViewModel

@{
    ViewData["Title"] = "Register";
}

<h2>Register</h2>

<h4>FranchiseViewModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Register" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label">Client Name</label>
                <input asp-for="Name" class="form-control" placeholder="Name" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FranchiseName" class="control-label"></label>
                <input asp-for="FranchiseName" class="form-control" />
                <span asp-validation-for="FranchiseName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Username" class="control-label"></label>
                <input asp-for="Username" class="form-control" />
                <span asp-validation-for="Username" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Phone" class="control-label"></label>
                <input asp-for="Phone" class="form-control" />
                <span asp-validation-for="Phone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Phone2" class="control-label"></label>
                <input asp-for="Phone2" class="form-control" />
                <span asp-validation-for="Phone2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Landline" class="control-label"></label>
                <input asp-for="Landline" class="form-control" />
                <span asp-validation-for="Landline" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Address" class="control-label"></label>
                <input asp-for="Address" class="form-control" />
                <span asp-validation-for="Address" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fax" class="control-label"></label>
                <input asp-for="Fax" class="form-control" />
                <span asp-validation-for="Fax" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Logo" class="control-label"></label>
                <input asp-for="Logo" type="file" accept="image/jpeg" class="form-control" />
                <span asp-validation-for="Logo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IATALicense" class="control-label"></label>
                <input asp-for="IATALicense" type="file" accept="image/jpeg" class="form-control" />
                <span asp-validation-for="IATALicense" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DTSLicense" class="control-label"></label>
                <input asp-for="DTSLicense" type="file" accept="image/jpeg" class="form-control" />
                <span asp-validation-for="DTSLicense" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NTNCertificate" class="control-label"></label>
                <input asp-for="NTNCertificate" type="file" accept="image/jpeg" class="form-control" />
                <span asp-validation-for="NTNCertificate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CNICPassport" class="control-label"></label>
                <input asp-for="CNICPassport" type="file" accept="image/jpeg" class="form-control" />
                <span asp-validation-for="CNICPassport" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Agreement" class="control-label"></label>
                <input asp-for="Agreement" type="file" accept="image/jpeg" class="form-control" />
                <span asp-validation-for="Agreement" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IsApproved" class="control-label"></label>
                <input asp-for="IsApproved" type="checkbox" checked class="form-control" />
                <span asp-validation-for="IsApproved" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IsRestrict" class="control-label"></label>
                <input asp-for="IsRestrict" type="checkbox" class="form-control" />
                <span asp-validation-for="IsRestrict" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

